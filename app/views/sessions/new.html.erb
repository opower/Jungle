<section class="orders-show">
  <header class="page-header">
  <h1>Login</h1>
  </header>
  <div class="order-summary jumbotron">
    <%= form_tag '/login' do %>

      Email: <%= text_field_tag :email %><br>
      Password: <%= password_field_tag :password %><br>
      <%= submit_tag "Submit" %>

    <% end %>
  </div>
</section>
